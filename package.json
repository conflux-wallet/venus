{
  "name": "venus",
  "version": "0.0.1",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "pod:install": "command -v pod && (cd ios/ && pod install && cd ..) || echo \"Skipping pod install\"",
    "clean:android": "rm -rf android/app/build",
    "clean:ios": "rm -rf ios/build",
    "clean:node": "rm -rf node_modules && yarn --frozen-lockfile",
    "clean": "yarn clean:ios && yarn clean:android && yarn clean:node",
    "ios": "react-native run-ios",
    "android": "react-native run-android  --active-arch-only",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write \"app/**/*.{js,jsx,json,md,ts,tsx}\"",
    "patch": "npx patch-package",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "expo:start": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web"
  },
  "dependencies": {
    "@nozbe/watermelondb": "^0.25.5",
    "@nozbe/with-observables": "^1.4.1",
    "expo": "^46.0.0",
    "expo-status-bar": "^1.4.4",
    "react": "18.0.0",
    "react-native": "0.69.2",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^13.1.2",
    "metro-react-native-babel-preset": "^0.70.3",
    "prettier": "^2.8.4",
    "react-test-renderer": "18.0.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --ignore-path=.prettierignore"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
